	year = sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE),
	note = meta$Version,
	url = "http://www.asasec.eu",
	publisher="Advisory System Against Sexual Exploitation of Children (ASASEC) Project (HOME/2010/ISEC/AG/043)",
